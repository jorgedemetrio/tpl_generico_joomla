{
  "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
  "name": "generico",
  "version": "1.0.0",
  "description": "Assets for the tpl_generico template.",
  "license": "GPL-2.0-or-later",
  "assets": [
    {
      "name": "tpl_generico.css",
      "type": "style",
      "uri": "template.css",
      "dependencies": [
        "bootstrap.css",
        "fontawesome"
      ]
    },
    {
      "name": "tpl_generico.js",
      "type": "script",
      "uri": "template.js",
      "dependencies": [
        "jquery",
        "core"
      ],
      "attributes": {
        "defer": true
      }
    },
    {
      "name": "tpl_generico.preset",
      "type": "preset",
      "dependencies": [
        "tpl_generico.css#style",
        "tpl_generico.js#script"
      ]
    },
    {
      "name": "tpl_generico.offline",
      "type": "style",
      "uri": "offline.css"
    },
    {
      "name": "fontawesome",
      "type": "style",
      "uri": "system/joomla-fontawesome.min.css"
    }
  ]
}
